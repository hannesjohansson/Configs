set term=builtin_ansi
syn on
set autoindent
colorscheme koehler
set tabstop=3
set shiftwidth=3
set encoding=utf8
set number
set ruler
set bs=2
set nocp
set ignorecase
set smartcase
set hlsearch
set incsearch
set history=1000
set undolevels=1000
set title
set visualbell
set noerrorbells
:set foldmethod=marker
:set foldmarker=/*,*/
